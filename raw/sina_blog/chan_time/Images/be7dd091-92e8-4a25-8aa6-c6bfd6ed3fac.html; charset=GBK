$(document).ready(function(){	
	$("#toggle .btn1").click(function () {
		$("#item_more").show();addCookie(1);
		$("#toggle .btn2").show();
		$(this).hide();
	});
	$("#toggle .btn2").click(function () {
		$("#item_more").hide();addCookie(0);
		$("#toggle .btn1").show();
		$(this).hide();
	});
	if (navigator.userAgent.search(/firefox/i) >= 0) {
    __firefox();
    }
	$("#own1").parent().mouseover(function () {
		$("#own1_txt").html($("#own1").html());
		$("#own1_box").show();
		$("#own1_btn").show();
	});
	$("#own1_btn").mouseout(function () {
		if (!checkparent("own1_box")&&!checkparent("own1_btn")) {
			$("#own1_box").hide();
			$("#own1_btn").hide();
		}
	});
	$("#own1_box").mouseleave(function () {	
		if(navigator.userAgent.indexOf("MSIE")>0) {   
			if (!checkparent("own1_box")&&!checkparent("own1_btn")) {
			$("#own1_box").hide();
			$("#own1_btn").hide();
			}
		}else{
			$("#own1_box").hide(); 
			$("#own1_btn").hide();
		}
	});
	$("#own3").parent().mouseover(function () {
		$("#own3_txt").html($("#own3").html());
		$("#own3_box").show();
		$("#own3_btn").show();
	});
	$("#own3_btn").mouseout(function () {
		if (!checkparent("own3_box")&&!checkparent("own3_btn")) {
			$("#own3_box").hide();
			$("#own3_btn").hide();
		}
	});
	$("#own3_box").mouseleave(function () {		
		if(navigator.userAgent.indexOf("MSIE")>0) {   
			if (!checkparent("own3_box")&&!checkparent("own3_btn")) {
			$("#own3_box").hide();
			$("#own3_btn").hide();
			}
		}else{
			$("#own3_box").hide();
			$("#own3_btn").hide();
		}
	});

	$("#own2").parent().mouseover(function () {
		$("#own2_txt").html($("#own2").html());
		$("#own2_box").show();
		$("#own2_btn").show();
	});
	$("#own2_btn").mouseout(function () {
		if (!checkparent("own2_box")&&!checkparent("own2_btn")) {
			$("#own2_box").hide();
			$("#own2_btn").hide();
		}
	});
	$("#own2_box").mouseleave(function () {		
		if(navigator.userAgent.indexOf("MSIE")>0) {   
			if (!checkparent("own2_box")&&!checkparent("own2_btn")) {
			$("#own2_box").hide();
			$("#own2_btn").hide();
			}
		}else{
			$("#own2_box").hide();
			$("#own2_btn").hide();
		}
	});
	
	$("#sort").mouseover(function () {
		var sort_html=$(this).html();
		if("默认排序"==sort_html)
		{
			$("#sort_coat li:first-child").hide();
		}
		else
		{
			$("#sort_coat li:first-child").show();
			for(var i=1;i<$("#sort_coat li").length;i++)
			{
				if($("#sort_coat li:nth-child("+i+") a").html()==sort_html)
				{
					$("#sort_coat li:nth-child("+i+")").attr("class","now");
					break;
				}
			}
		}
		$("#sort_coat").show();
	});
	$("#sort").mouseout(function () {
		if (!checkparent("sort_coat")) {
		    $("#sort_coat").hide();
		}
	});
	$("#sort_coat").mouseleave(function () {
		$("#sort_coat").hide();
	});
});

function __firefox() {
    HTMLElement.prototype.__defineGetter__("runtimeStyle", __element_style);
    window.constructor.prototype.__defineGetter__("event", __window_event);
    Event.prototype.__defineGetter__("srcElement", __event_srcElement);
    Event.prototype.__defineGetter__("fromElement", __event_fromElement);
    Event.prototype.__defineGetter__("toElement", __event_toElement);
    Event.prototype.__defineGetter__("x", __event_x);
    Event.prototype.__defineGetter__("y", __event_y);
    HTMLElement.prototype.__defineGetter__("children",
    function() {
        var b = new Object();
        var c = 0;
        for (var a = 0; a < this.childNodes.length; a++) {
            if (this.childNodes[a].nodeType == 1) {
                b[c] = this.childNodes[a];
                c++
            }
        }
        b.length = c;
        return b
    })
}
function __element_style() {
    return this.style
}
function __window_event() {
    return __window_event_constructor()
}
function __event_srcElement() {
    return this.target
}
function __event_fromElement() {
    var a;
    if (this.type == "mouseover") {
        a = this.relatedTarget
    } else {
        if (this.type == "mouseout") {
            a = this.target
        }
    }
    if (!a) {
        return
    }
    while (a.nodeType != 1) {
        a = a.parentNode
    }
    return a
}
function __event_toElement() {
    var a;
    if (this.type == "mouseout") {
        a = this.relatedTarget
    } else {
        if (this.type == "mouseover") {
            a = this.target
        }
    }
    if (!a) {
        return
    }
    while (a.nodeType != 1) {
        a = a.parentNode
    }
    return a
}
function __event_x() {
    return this.pageX
}
function __event_y() {
    return this.pageY
}
function __window_event_constructor() {
    if (document.all) {
        return window.event
    }
    var c = __window_event_constructor.caller;
    while (c != null) {
        var b = c.arguments[0];
        if (b) {
            var a = b.constructor;
            if (a.toString().indexOf("Event") != -1) {
                return b
            }
        }
        c = c.caller
    }
    return null
}


function checkparent(a) {
    var b = document.getElementById(a);
    var c = event.toElement;
    while (c) {
        if (c == b) {
            return true
        }
        c = c.parentNode
    }
    return false
}
</script>

<script>
window.tabCity=function(id){
                this.id=$("#"+id);
        };
        tabCity.prototype={
                eventInit:function(){
                        var city=this.id.find(".open");
                        this.id.find(".default").mouseenter(function(){
                                city.show();
                        })
                        city.mouseleave(function(){
                                city.hide();
                        });
                        this.id.find(".more .r a").click(function(){
                                city.hide();
                                this.blur();
                                return false;
                        });
                }
        };

</script>

