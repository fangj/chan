				<script type="text/javascript">
					function addCookie(v) {
						var str = "search=" + v;
						document.cookie = str;
					}

					function getCookie(objName) {//获取指定名称的cookie的值
						var arrStr = document.cookie.split("; ");
						for ( var i = 0; i < arrStr.length; i++) {
							var temp = arrStr[i].split("=");
							if (temp[0] == objName)
								return unescape(temp[1]);
						}
					}
					function showMore(bl) {
						document.getElementById('item_more').style.display = bl ? 'block'
								: 'none';
						var spans = document.getElementById('toggle')
								.getElementsByTagName('div');
						var len = spans.length;
						for ( var i = 0; i < len; i++) {
							if (spans[i].className == 'btn2') {
								spans[i].style.display = bl ? 'block' : 'none';
							} else if (spans[i].className == 'btn1') {
								spans[i].style.display = bl ? 'none' : 'block';
							}
						}
					}

					showMore(getCookie("search") == 1);
					function showBP(key) {
						var blist_tab = document.getElementById('brandTab');
						var list = blist_tab.getElementsByTagName('span');
						var len = list.length;
						for ( var i = 0; i < len; i++) {
							if (list[i].key == key) {
								list[i].className = 'now';
							} else {
								if (list[i].className != 'unuse')
									list[i].className = '';
							}
						}
						var div = document.getElementById("blist_content");
						var divs = div.getElementsByTagName("div");
						var len = divs.length;
						var notShow = true;
						for ( var i = 0; i < len; i++) {
							var d = divs[i];
							if (d.id && d.id.indexOf('blist_') == 0) {
								if (d.id == 'blist_' + key) {
									notShow = false;
									d.style.display = 'block';
								} else {
									d.style.display = 'none';
								}
							}
						}
						if (notShow) {
							document.getElementById('blist_null').style.display = 'block';
						}
					}

					var blist_tab = document.getElementById('brandTab');
					var list = blist_tab.getElementsByTagName('span');
					var len = list.length;
					for ( var i = 0; i < len; i++) {
						var tt = list[i].innerHTML;
						if (tt == '热门') {
							tt = 'rm';
						}
						list[i].key = tt;
						if (document.getElementById('blist_' + tt)) {
							list[i].onmouseover = function() {
								showBP(this.key);
							}
						} else {
							list[i].className = "unuse";
						}
					}
				</script>




