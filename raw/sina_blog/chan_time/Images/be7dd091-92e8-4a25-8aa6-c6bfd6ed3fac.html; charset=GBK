<script type="text/javascript">
String.prototype.trim=function(){ return this.replace(/(^\s*)|(\s*$)/g, "");}

function doSearchPrice(){ 
	 var baseUrl = '/price/home.shtml?paixu=0&item=';
	 var minPrice = document.getElementById("zdyPrice1").value.trim();
	 var maxPrice = document.getElementById("zdyPrice2").value.trim();
	 if(minPrice.length==0&&maxPrice.length==0){
	       alert("请输入价格范围"); return;
	 }
	 var regNum =/^[0-9]{1,}(\.[0-9]{1,})*$/; 
     if(minPrice.length>0&&!regNum.test(minPrice)){
            alert("价格必须是数值类型");return;
     }
 	 if(maxPrice.length>0&&!regNum.test(maxPrice)){
           alert("价格必须是数值类型");return;
     }
     if(maxPrice.length>0&&minPrice.length>0){
            maxPrice =  Number(maxPrice);
            minPrice = Number(minPrice);
            if(maxPrice<minPrice){
                 var d = maxPrice;
                 maxPrice = minPrice;
                 minPrice = d;
            }
     }
	 document.location.href = baseUrl+'price:'+minPrice+"-"+maxPrice+'#condi';
}
</script>
