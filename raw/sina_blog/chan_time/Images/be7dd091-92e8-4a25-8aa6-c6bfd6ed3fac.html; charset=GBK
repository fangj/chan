<script src="/epicm/allmodelstatus.sip"></script>
<script>

function getModelStatus(m){
     if(typeof(allstatus) != "undefined"){
        var arr = allstatus.split(",");
        for(var i=1;i<arr.length;i+=2){
             if(arr[i-1] == m){
                 return arr[i];
             }
        }
    }
    return null;
}

function booleanModelStatus(st,t){
 
     if(st != null){
          var mask = 1 << (t - 1); 
          if((st & mask) > 0){
               return true;
          }
     }
     return false;
}

function setNavUrl(mid){ //add by qzl 设置导航的url 
	var st = getModelStatus(mid);  
	if(booleanModelStatus(st,2)){
		   document.getElementById('nav_photo').href = '/model_'+mid+'/picture.shtml'; 
    }   
	if(booleanModelStatus(st,3)){
		 // document.getElementById('nav_price').href = '/model_'+mid+'/price.shtml'; 
    }
	if(booleanModelStatus(st,6)){ 
        document.getElementById('nav_cxdata').href = '/cxdata/index.html?page=/model_'+mid+'/salescar.shtml&model='+mid;
	}
	if(booleanModelStatus(st,11)){
		   document.getElementById('nav_satisfy').href = '/model_'+mid+'/satisfy.shtml';  
	}
	if(booleanModelStatus(st,3)){
		  document.getElementById('nav_priceInfo').href = '/model_'+mid+'/priceInfo.shtml';  
	}
	if(booleanModelStatus(st,8)){
		  document.getElementById('nav_video').href = '/video/model_'+mid+'.shtml';  
	}
} 

</script>   



