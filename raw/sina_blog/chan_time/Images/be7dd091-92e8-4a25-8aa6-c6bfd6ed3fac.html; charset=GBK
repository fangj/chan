<script type="text/javascript">  
function SIP_AddToBasketD(e) {
 var evt = e || window.event;
 var evtTarget = evt.target || evt.srcElement;

 var ID = evtTarget.id;
 var NAME = evtTarget.alt;
 var _ID = ID.substring(6);
 SIPAUTO_basket.store(_ID,NAME);
} 
